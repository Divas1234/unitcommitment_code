{
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "name": "One Dark italic",
                "scope": [
                    "comment",
                    "entity.other.attribute-name",
                    "keyword",
                    "markup.underline.link",
                    "storage.modifier",
                    "storage.type",
                    "string.url",
                    "variable.language.super",
                    "variable.language.this"
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "name": "One Dark italic reset",
                "scope": [
                    "keyword.operator",
                    "keyword.other.type",
                    "storage.modifier.import",
                    "storage.modifier.package",
                    "storage.type.built-in",
                    "storage.type.function.arrow",
                    "storage.type.generic",
                    "storage.type.java",
                    "storage.type.primitive"
                ],
                "settings": {
                    "fontStyle": ""
                }
            }
        ],
        "comments": ""
    },
    "terminal.integrated.fontSize": 14,
    "code-runner.clearPreviousOutput": true,
    "terminal.integrated.fontLigatures": true,
    "terminal.integrated.mouseWheelZoom": true,
    "terminal.integrated.fontFamily": "Geist Mono, Bahnschrift",
    "julia.focusPlotNavigator": true,
    "editor.fontSize": 14,
    "editor.fontFamily": "'Droid Sans Mono', Geist Mono, Fira Code, 'Hack Nerd Font', Menlo, Monaco, 'Courier New', monospace",
    "commentAnchors.tagHighlights.enabled": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": true,
    "BracketHighlighter.blurOutOfScopeText": false,
    "BracketHighlighter.differentSymbolHighlightingUsed": false,
    "BracketHighlighter.ignoreContent": false,
    "BracketHighlighter.backgroundColor": "",
    "BracketHighlighter.backgroundColorSymbol": "",
    "BracketHighlighter.border": "1px dotted blue",
    "BracketHighlighter.borderSymbol": "",
    "BracketHighlighter.textColorSymbol": "",
    "BracketHighlighter.textColor": "",
    "BracketHighlighter.fontWeight": "",
    "BracketHighlighter.isInsideOfClosingSymbolIgnored": false,
    "BracketHighlighter.isInsideOfOpeningSymbolIgnored": true
}